{
  "manifest_version": 2,

  "name": "FB Event to gCal Event",
  "description": "Imports facebook events into google calendar.",
  "version": "1.0",

  "permissions": [
    "https://facebook.com/", "http://*.google.com/", "https://*.google.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}