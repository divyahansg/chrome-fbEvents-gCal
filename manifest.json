{
  "manifest_version": 2,

  "name": "FB Event to gCal Event",
  "description": "Imports facebook events into google calendar.",
  "version": "1.0",

  "permissions": [
    "activeTab","https://facebook.com/", "http://*.google.com/", "https://*.google.com/"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
	"matches" : ["http://www.facebook.com/*"],
    	"js": ["jquery-latest.min.js", "script.js"]
    }
  ]
}